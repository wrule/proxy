{
  "openapi": "3.1.0",
  "info": {
    "title": "向量搜索查询",
    "description": "根据关键词在指定类型的数据中进行向量搜索，支持产品、脚本、计划、目标、压测记录、测试报告、定时任务等多种数据类型的智能搜索",
    "version": "v1.0.0"
  },
  "servers": [
    {
      "url": "http://192.168.8.139:3000"
    }
  ],
  "paths": {
    "/api/xsea/vector/query": {
      "post": {
        "description": "使用AI生成的关键词在指定数据类型中进行向量搜索，返回相关的数据列表",
        "operationId": "VectorQuery",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "type": {
                    "type": "string",
                    "enum": ["PRODUCT", "SCRIPT", "PLAN", "GOAL", "RECORD", "REPORT", "SCHEDULE"],
                    "description": "搜索的数据类型：PRODUCT(产品)、SCRIPT(脚本)、PLAN(计划)、GOAL(目标)、RECORD(压测记录)、REPORT(测试报告)、SCHEDULE(定时任务)"
                  },
                  "text": {
                    "type": "string",
                    "description": "搜索关键词，可以是一个或多个关键词，由AI自动生成"
                  }
                },
                "required": ["type", "text"]
              }
            }
          }
        },
        "deprecated": false
      }
    }
  },
  "components": {
    "schemas": {
      "VectorQueryRequest": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string",
            "enum": ["PRODUCT", "SCRIPT", "PLAN", "GOAL", "RECORD", "REPORT", "SCHEDULE"],
            "description": "数据类型"
          },
          "text": {
            "type": "string",
            "description": "搜索关键词"
          }
        },
        "required": ["type", "text"]
      }
    }
  }
}